pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
 game = {hs=0}
 reset()
end

function reset()
 pdl = {pos=64, w=16}
 ball = {x=64, y=64, w=4, h=4,
         dx=3, dy=-3}
 game.over = false
 game.won = false
 game.hit = 0
 game.score = 0 
 brick = {}
 shot = {}
 for row = 0,3 do
  for col = 0,7 do
   add(brick, {x=col * 16,
               y=20 + (row * 6),
               w=14,
               h=2,
               c=row + 8})
  end
 end
end

function _draw()
 cls()
 rectfill(pdl.pos, 124,
          pdl.pos + pdl.w, 128, 7)
 spr(1, ball.x, ball.y)
 for s in all(shot) do
  spr(2, s.x, s.y)
 end
 
 for b in all(brick) do
  rectfill(b.x, b.y, b.x + b.w, b.y + b.h, b.c)
 end
 
 if game.over then
  print("game over", 0, 8, 7)
 elseif game.won then
  print("you win!!!", 0, 8, 7)
 end
 print("score: "..game.score, 0, 0, 7)
 local hs = "high: " .. game.hs
 print(hs, 128 - #hs * 4, 0)
end

function _update()

 if (game.over or game.won) then
  if (game.score > game.hs) game.hs = game.score
  if (btnp(5)) reset()
  return
 end
 
 if btnp(4) then
  add(shot, {x=pdl.pos+8, y=124, w=2, h=4})
 end
 
 if (btn(0)) pdl.pos -= 6
 if (btn(1)) pdl.pos += 6
 
 if (pdl.pos < 0) pdl.pos = 0
 if (pdl.pos + pdl.w > 128) pdl.pos = 128 - pdl.w

 ball.x += ball.dx
 ball.y += ball.dy
 
 if ball.y > 125 then
  game.over = true
  return
 end
 
 if ball.x < pdl.pos + pdl.w and
    ball.x + ball.w > pdl.pos and
    ball.y + ball.h >= 124 then
  ball.dy = -3
  local dir = sgn(ball.dx)  
  local bounce = ((pdl.pos + pdl.w - ball.x) - 1) / 3
  if bounce == 0 then ball.dx = 5
   elseif bounce == 1 then ball.dx = 4
   elseif bounce == 2 then ball.dx = dir * 3
   elseif bounce == 3 then ball.dx = -4
   else ball.dx = -5
  end
 end
 
 if ball.x + ball.w >= 128 or
    ball.x <= 0 then
  ball.dx = -ball.dx
 end
 
 if ball.y <= 0 then
  ball.dy = -ball.dy
 end
 
 local blp = false
 for b in all(brick) do
  if (coll(b, ball)) del(brick, b) blp=true game.score += (b.c - 7)
 end
 if (blp) ball.dy = -ball.dy
 
 for s in all(shot) do
  s.y -= 3
  for b in all(brick) do
   if coll(s, b) then
    del(shot, s)
    del(brick, b)
   end
  end
 end
 
 if #brick == 0 then
  game.won = true
 end
end

function coll(a, b)
 if a.x < b.x + b.w and
    a.x + a.w > b.x and
    a.y < b.y + b.h and
    a.y + a.h > b.y then
  return true
 end
 
 return false
end
