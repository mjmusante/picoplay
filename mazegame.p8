pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _update()
 if (pl.last <= 15) pl.last += 1
 canmove += 1
 
 for s in all(shots) do
  s.x += s.d.x * 5
  s.y += s.d.y * 5
  if (s.x < 0 or s.x > 128) del(shots, s)
  if (s.y < 0 or s.y > 128) del(shots, s)

  f = {x=flr(s.x/8), y=flr(s.y/8)}
  
  m = isin(maze, f)
  
  if m != false then
   del(maze, m)
   del(shots, s)
  end
 end
 
 local foo = {x=pl.x, y=pl.y}
 if canmove > 3 then
  canmove = 0
	 if btn(0) then
	  pl.x -= 1
	  pl.sp = pl.left
	  pl.d = {x=-1, y=0, s=3}
	 elseif btn(1) then
	  pl.x += 1
	  pl.sp = pl.right
	  pl.d = {x=1, y=0, s=3}
	 elseif btn(2) then
	  pl.y -= 1
	  pl.sp = pl.up
	  pl.d = {x=0, y=-1, s=2}
	 elseif btn(3) then
	  pl.y += 1
	  pl.sp = pl.down
	  pl.d = {x=0, y=1, s=2}
	 end
	 if (isin(maze, pl)) then
	  pl.x = foo.x
	  pl.y = foo.y
	 else
	  if (pl.x > 15) pl.x = 15
	  if (pl.x < 0) pl.x = 0
	  if (pl.y > 15) pl.y = 15
	  if (pl.y < 0) pl.y = 0
	 end
 end
 
 if btnp(4) or (btn(4) and pl.last > 15) then
  fire()
  pl.last = 0
 end
end

function fire()
 add(shots, {x=pl.x * 8, y=pl.y * 8, s=pl.d.s, d=pl.d})
end

function _draw()
 cls()
 for m in all(maze) do
  spr(1, m.x * 8, m.y * 8)
 end
 spr(pl.sp, pl.x * 8, pl.y * 8)
 for s in all(shots) do
  spr(s.s, s.x, s.y)
 end
end

function gen_maze()
 maze = {}
 shots = {}
 for i = 1,10 do
  add(maze, {x=rand(16), y=rand(16)})
 end
end

function rand(x)
 return flr(rnd(x))
end

function _init()
 f = false
 canmove = 0
 gen_maze()
 pl = {x = 0, y = 0, sp=19,
       left=17, right=18,
       up=16, down=19, last=0,
       d={x=1, y=0, s=3}}
 local foo = 0
 while (foo == 0) do
  if isin(maze, pl) then
   pl.x += 1
   if pl.x > 15 then
    pl.y += 1
    pl.x = 0
   end
  else
   foo = 1
  end
 end
end

function isin(ary, i)
 for x in all(ary) do
  if x.x == i.x and x.y == i.y then
    return x
  end
 end
 return false
end
